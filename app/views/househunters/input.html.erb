<%=form_tag(gotosearch_path, :method => "get") do %>
  <%= label_tag(:location, "location:") %>
  <%= text_field_tag(:location) %><br>
  <%= label_tag(:area, "Area:") %>
  <%= text_field_tag(:area) %><br>
  <%= label_tag(:year_built, "year:") %>
  <%= text_field_tag(:year_built)%><br>
  <%= label_tag(:style, "style:") %>
  <%= text_field_tag(:style) %><br>
  <%= label_tag(:list_prize, "list prize:") %>
  <%= text_field_tag(:list_prize) %><br>
  <%= label_tag(:floor_count, "floor count") %>
  <%= text_field_tag(:floor_count) %><br>
  <%= label_tag(:basement, "basement enter value as true or false:") %>
  <%= text_field_tag(:basement) %><br>
  <%= label_tag(:owner_name, "owner name") %>
  <%= text_field_tag(:owner_name) %><br>

  <%= submit_tag("Search") %>
<% end %>