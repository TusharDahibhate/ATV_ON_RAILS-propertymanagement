<h1>Potential buyers</h1>

<table>
  <thead>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Phone</th>
    <th>Contact method</th>
    <th>User</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @fu.each do |k,v| %>
    <% v.each do |f| %>
      <tr>
        <td><%=f.first_name %></td>
        <td><%=f.last_name %></td>
        <td><%=f.phone %></td>
        <td><%=f.contact_method %></td>
      </tr>
    <%end %>
  <% end %>
  </tbody>
</table>

<br>